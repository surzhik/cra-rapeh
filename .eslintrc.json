{
  "extends": [
  "airbnb",
  "prettier",
  "prettier/react",
  "plugin:prettier/recommended",
  "eslint-config-prettier"
],
  "parser": "babel-eslint",
  "rules": {
  "react/display-name": 0,
  "react/prop-types": 1,
  "react/jsx-no-undef": 2,
  "no-console": 2,
  "no-unused-vars": 2,
  "import/no-unresolved": "off",
    "react/jsx-filename-extension": [
    1,
    {
      "extensions": [".js", ".jsx"]
    }
  ],
    "prettier/prettier": [
    "error",
    {
      "trailingComma": "es5",
      "singleQuote": true,
      "printWidth": 120,
      "bracketSpacing": true,
      "tabWidth": 2
    }
  ]
},
  "plugins": ["prettier"]
}
